{
  "name": "retext-lexrank",
  "version": "0.0.2",
  "description": "Lexrank algorithm for retextjs",
  "repository": "https://github.com/gorango/retext-lexrank",
  "type": "module",
  "sideEffects": false,
  "main": "src/index.js",
  "author": {
    "name": "Goran Spasojevic",
    "email": "gospaso@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "scripts": {
    "dev": "nodemon sandbox/example",
    "test:dev": "npx nodemon --conditions development test.js",
